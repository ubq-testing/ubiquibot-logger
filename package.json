{
  "name": "@ubiquity-dao/ubiquibot-logger",
  "version": "1.1.1",
  "description": "Ubiquity logging module with Supabase support.",
  "author": "Ubiquity DAO",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "husky install",
    "format": "run-s format:*",
    "format:prettier": "prettier --write .",
    "format:eslint": "eslint --fix .",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "logging",
    "supabase",
    "dao",
    "ubiquity",
    "open-source"
  ],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-md-dashboard": "^0.8.0",
    "knip": "^3.3.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier --write",
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "yarn@1.22.22"
}